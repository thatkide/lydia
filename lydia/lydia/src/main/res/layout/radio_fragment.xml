<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
			  android:orientation="vertical"
			  android:layout_width="match_parent"
			  android:layout_height="match_parent">
	<Button android:id="@+id/radio_seek_up" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Seek up"/>
	<Button android:id="@+id/radio_seek_down" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Seek down"/>
	<Button android:id="@+id/radio_fox" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="FOX"/>
	<Button android:id="@+id/radio_101" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Rock 101"/>
</LinearLayout>
